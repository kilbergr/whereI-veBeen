<% include ../partials/header %>
<h1>See all the places!</h1>

<a href="/places/new">Add a new place</a>

<% places.forEach(function(place){ %>
  <p>
    <a href="/places/<%= place._id%>/"><%= place.address%></a>
  </p>
  <p>
    <a href="https://www.google.com/maps/@<%=place.lat%>,<%=place.long%>,12z"><%= place.lat %>,<%= place.long %></a>
  </p>
  <p>
    <a href="/places/<%= place.id%>/edit">Edit</a>
  </p>



<% })%>

<% include ../partials/footer %>